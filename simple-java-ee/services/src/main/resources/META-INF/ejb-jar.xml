<ejb-jar
    id = "br.com.cpqd.framework.satellite.jpa.persistence.EntityDAO"
    xmlns = "http://java.sun.com/xml/ns/javaee"
    xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee
                            http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
    version = "3.1">
    <description>Hello Service Bean - WebLogic Test</description>
    <display-name>Hello Service Bean</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>jms.simple.javaee.services.HelloServiceBean</ejb-name>
            <ejb-class>jms.simple.javaee.services.HelloServiceBean</ejb-class>
            <session-type>Stateless</session-type>
        </session>
    </enterprise-beans>
</ejb-jar>
